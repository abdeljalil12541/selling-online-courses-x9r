{
  "version": 2,
  "builds": [
    {
      "src": "sellCoursesOnline/wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "sellCoursesOnline/wsgi.py" }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "sellCoursesOnline.settings",
    "PYTHONUNBUFFERED": "true",
    "VERCEL": "1"
  }
}
